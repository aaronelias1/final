@model Redes.Models.Login

@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm())
{
    <body>
        <div class="container">
            <div id="loginbox" style="margin-top:50px;" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <div class="panel-title">Ingresar</div>
                    </div>
                    <div style="padding-top:30px" class="panel-body">
                        <div style="display:none" id="login-alert" class="alert alert-danger col-sm-12"></div>

                        <form id="loginform" class="form-horizontal" role="form">

                            <img src="data:image/png;base64,"
                                 width="1000" height="200" class="col-xs-4  " />

                            <div style="margin-bottom: 20px" class="input-group">

                                <span class="input-group-addon" id="sizing-addon1"><i class="glyphicon glyphicon-user"></i></span>
                                @Html.TextBoxFor(m => m.correo, new { htmlAttributes = new { @class = "form-control" } })
                                <td>@Html.ValidationMessageFor(m => m.correo)</td>
                            </div>

                            <div style="margin-bottom: 25px" class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                                @Html.PasswordFor(m => m.contraseña, new { htmlAttributes = new { @class = "form-control" } })
                                <td>@Html.ValidationMessageFor(m => m.contraseña)</td>
                            </div>

                            <center>
                                <div class="col-xs-4">
                                    <div class="checkbox">
                                        <label class"col-xs-4">
                                            <input id="login-remember" type="checkbox" name="remember" value="1"> Recordad
                                        </label>
                                    </div>
                                </div>

                                <div style="margin-top:10px" class="form-group">
                                    <!-- Button -->
                                    <div class="col-sm-12 controls">
                                        <button type="submit" id="btn-login" class="btn btn-success">Entrar</button>

                                        <a id="btn-fblogin" href="@Url.Action("Registrar","Registro")" class="btn btn-primary">registro</a>
                                    </div>
                                </div>
                            </center>
                        </form>
                        @ViewBag.Message
                    </div>
                </div>
            </div>






    </body>

}

